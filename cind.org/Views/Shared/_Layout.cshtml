<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - cind.org actions toward decreasing Impact of substance abuse in Nigeria</title>

    <link href="~/Content/materialize/css/materialize.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Open+Sans|Quicksand" rel="stylesheet">
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/ionicons-2.0.1/css/ionicons.min.css" rel="stylesheet" />
    <link href="~/Content/animate.css" rel="stylesheet" />

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!--Scripts-->
    <script src="~/Scripts/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/materialize/materialize.min.js"></script>

</head>
<body>
    <div>
        <nav class="top-nav">
            <div class="nav-wrapper">
                <a href="#" class="brand-logo">Logo</a>
                <span class="top-nav-title animated fadeInDown hide-on-med-and-down">Community Intervention Network on Drugs (CIND)</span>
                <ul id="nav-mobile" class="right hide-on-med-and-down">
                    <li><a href="#"><i class="ion ion-social-twitter-outline"></i></a></li>
                    <li><a href="#"><i class="ion ion-social-facebook-outline"></i></a></li>
                    <li><a href="#"><i class="ion ion-social-googleplus-outline"></i></a></li>
                </ul>
            </div>
        </nav>
        <nav class="top-sub-nav hide-on-med-and-down">
            <div class="nav-wrapper">
                <ul id="nav-mobile" class="center hide-on-med-and-down">
                    <li>
                        <a href="#">
                            <i class="ion ion-ios-photos-outline"></i>&nbsp;General Information
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="ion ion-ios-briefcase-outline"></i>&nbsp;Organizational Structure
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="ion ion-ios-list-outline"></i>&nbsp;About CIND
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="ion ion-ios-people-outline"></i>&nbsp;Key Personnel
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="ion ion-ios-unlocked-outline"></i>&nbsp;Thematic Areas
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="ion ion-ios-clock-outline"></i>&nbsp;Programs
                        </a>
                    </li>

                </ul>
            </div>
        </nav>
        @RenderBody()

        <footer class="hide-on-med-and-down">
            <div class="row">
                <div class="col s4 quicklinks">
                    <h4>Quick Links</h4><br />
                    <span><a href="#"><i class="ion ion-ios-circle-outline"></i>General Information</a></span>
                    <span><a href="#"><i class="ion ion-ios-circle-outline"></i>Conferences & Programs</a></span>
                    <span><a href="#"><i class="ion ion-ios-circle-outline"></i>About the CIND Community</a></span>
                    @*<l1></l1>*@

                </div>
                <div class="col s4">
                    <h4>Contacts</h4><br />
                    <p><b>Email: </b>contact@cind.org</p>
                </div>
                <div class="col s4"></div>
            </div>
        </footer>
        <p class="copyright">
            <span>
                <a href="#"><i class="ion ion-social-twitter-outline"></i></a>
                <a href="#"><i class="ion ion-social-facebook-outline"></i></a>
                <a href="#"><i class="ion ion-social-googleplus-outline"></i></a>
            </span>
            &copy; @DateTime.Now.Year - Copyright Cind.org. all rights reserved<br /><br />
            <span style="font-size:8pt">Powered by <a style="font-size:8pt" href="http://digitalforte.ng">Digital Forte Enterprise Systems Ltd.</a></span>
        </p>

    </div>



    @RenderSection("scripts", required: false)
</body>
</html>
